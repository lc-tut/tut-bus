{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "lint": {
      "dependsOn": ["^lint"]
    },
    "fix": {
      "dependsOn": ["^fix"]
    },
    "build": {
      "dependsOn": ["//#prebuild"],
      "outputs": ["dist/**", "out/**", ".next/**", "!.next/cache/**"]
    },
    "typecheck": {
      "dependsOn": ["^typecheck"]
    }
  }
}
