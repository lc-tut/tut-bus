{
  "name": "spec",
  "private": true,
  "scripts": {
    "lint": "tsp format . --check",
    "fix": "tsp format .",
    "build": "tsp compile .",
    "gen:ts": "autorest ./readme.ts.yaml --typescript",
    "gen:go": "mkdir -p ../api/models && oapi-codegen --config=./oapi-codegen.yaml ./dist/openapi.yaml"
  },
  "devDependencies": {
    "@autorest/go": "4.0.0-preview.72",
    "@typespec/compiler": "^1.0.0",
    "@typespec/http": "^1.0.1",
    "@typespec/openapi3": "^1.0.0",
    "autorest": "^3.7.1",
    "npm-run-all": "^4.1.5"
  }
}
